
type Numeric = enum {
    Int(i32),
    Float(float)
};

fn numeric(i32 i) -> Numeric {
    match i {
        0 => Numeric::Int(1)
        _ => Numeric::Float(1.0)
    }
}

pub fn sut() -> i32 {
    let left = match numeric(0) {
        Numeric::Int(x) => x
        Numeric::Float(_) => 0
        _ => 10
    };

    let i32 right = match numeric(1) {
        Numeric::Int(_) => 0
        Numeric::Float(x) => 1
        _ => 10
    };

    left + right
}
