pub impure fn malloc(i32 size) -> i8*;
pub impure fn printf(str fmt, *) -> i32;

type Thing = struct {
    i8[5] name;
};

pub impure fn main() -> i32 {
    let mut thing = as Thing* malloc(sizeof Thing);
    thing->name[0] = 'H';
    thing->name[1] = 'i';
    thing->name[2] = 0;
    printf("hi: %s\n", thing->name);
    0
}
